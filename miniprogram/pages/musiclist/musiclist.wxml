<!--pages/musiclist/musiclist.wxml-->
<view class="'detail-container" style='background:url({{listInfo[0].picUrl}}) no-repeat top/cover'></view>
<view class="detail-mask"></view>
<view class="detail-info">
    <image src="{{listInfo[0].picUrl}}" class='detail-img' mode="widthFix"></image>
    <view class="detail">
        <view class="detail-nm">{{listInfo[0].name}}</view>
    </view>
</view>
<x-musiclist musiclist="{{musiclist}}"></x-musiclist>